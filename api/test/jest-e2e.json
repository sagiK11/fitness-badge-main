{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^csv-parse/sync": "<rootDir>/../node_modules/csv-parse/dist/cjs/sync.cjs",
    "^@src/(.*)$": "<rootDir>/../src/$1"
  }
}
